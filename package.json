{
	"name": "agora",
	"version": "1.0.0",
	"description": "A native application built with NativeScript-Vue",
	"author": "Ben van Heerden <benshez1@gmail.com>",
	"license": "MIT",
	"scripts": {
		"web:start": "webpack-dev-server --mode development --inline --hot --config build/web/webpack.config.js --history-api-fallback",
		"dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot --config ./build/build/web/webpack.config.js --mode development",
		"web:dev": "webpack --mode development --config build/web/webpack.config.js",
		"web:build": "webpack --mode production --config build/web/webpack.config.js",
		"all:clean": "rimraf node_modules && rimraf hooks && rimraf platforms && npm install",
		"web:clean": "rimraf platforms/web",
		"web:run:local": "cross-env NODE_ENV=development webpack-dev-server --open --hot --config ./build/webpack.base.conf.js --mode development --platform=web --environment=development",
		"web:build:development": "cross-env NODE_ENV=development webpack --progress --hide-modules --config ./build/webpack.base.conf.js --mode development --platform=web --environment=development",
		"web:build:test": "cross-env NODE_ENV=test webpack --progress --hide-modules --config ./build/webpack.base.conf.js --mode development --platform=web --environment=test",
		"web:build:production": "cross-env NODE_ENV=production webpack --progress --hide-modules --config ./build/webpack.base.conf.js --mode production --platform=web --environment=production",
		"android:clean": "rimraf platforms/android",
		"android:run:local": "tns run android --bundle",
		"android:build:development": "tns build android --bundle"
	},
	"nativescript": {
		"id": "com.agora.application",
		"tns-ios": {
			"version": "5.1.1"
		},
		"tns-android": {
			"version": "5.1.0"
		}
	},
